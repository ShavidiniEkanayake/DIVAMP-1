<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_login_background"
    tools:context=".location.Registration_2">

    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="@drawable/ic_email"
        android:ems="10"
        android:fontFamily="@font/abhaya_libre_extrabold"
        android:inputType="textEmailAddress"
        android:paddingLeft="30dp"
        android:text="E-mail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactNumber" />

    <Button
        android:id="@+id/finishbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="184dp"
        android:layout_marginTop="524dp"
        android:background="@drawable/ic_finish_btn"
        android:foreground="@drawable/ic_finish_btn"
        android:text="Button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/fullName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/ic_registration_text"
        android:ems="10"
        android:fontFamily="@font/abhaya_libre_extrabold"
        android:inputType="textPersonName"
        android:paddingLeft="30dp"
        android:text="@string/Owner_full_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/NIC" />

    <EditText
        android:id="@+id/NIC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="284dp"
        android:background="@drawable/ic_registration_text"
        android:ems="10"
        android:fontFamily="@font/abhaya_libre_extrabold"
        android:inputType="textPersonName"
        android:paddingLeft="30dp"
        android:text="@string/Owner_NIC"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="175dp"
        android:layout_height="61dp"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/yantramanav_bold"
        android:text="Location"
        android:textColor="#FFFFFF"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="93dp"
        android:layout_height="41dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/yantramanav_bold"
        android:text="Portal"
        android:textColor="#F26421"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="233dp"
        android:layout_height="34dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="156dp"
        android:fontFamily="@font/yantramanav_bold"
        android:text="Self Registration"
        android:textColor="#000000"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/contactNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/ic_registration_text"
        android:ems="10"
        android:fontFamily="@font/abhaya_libre_extrabold"
        android:inputType="phone"
        android:paddingLeft="30dp"
        android:text="@string/Phone_number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fullName" />
</androidx.constraintlayout.widget.ConstraintLayout>